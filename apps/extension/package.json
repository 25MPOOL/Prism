{
  "name": "extension",
  "displayName": "",
  "version": "0.0.1",
  "description": "Prism",
  "author": "25M POOL",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "key": "",
    "permissions": [
      "identity",
      "storage",
      "sidePanel",
      "activeTab",
      "commands"
    ],
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "commands": {
      "_execute_side_panel": {
        "suggested_key": {
          "default": "Alt+S",
          "mac": "Alt+S",
          "windows": "Alt+S",
          "chromeos": "Alt+S",
          "linux": "Alt+S"
        },
        "description": "サイドパネルを開閉する"
      }
    }
  }
}
